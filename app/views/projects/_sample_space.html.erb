<%= javascript_tag do%>

 var grid;
  var columns = [
    {id: "title", name: "Title", field: "name"},
    {id: "created_at", name: "Created_at", field: "created_at"},
    {id: "protocole", name: "Protocole", field: "protocole"},
    {id: "description", name: "Description", field: "description"}
  ];

  var datum = <%= raw @project.samples.to_json %>;
  var options = {
    enableCellNavigation: true,
    enableColumnReorder: false
  };
$( document ).ready(function() {
    grid = new Slick.Grid("#SampleGrid", datum, columns, options);
});

<% end %>

