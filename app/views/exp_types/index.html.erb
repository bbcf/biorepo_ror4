<p id="notice"><%= notice %></p>

<h1>Listing Exp Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exp_types.each do |exp_type| %>
      <tr>
        <td><%= link_to 'Show', exp_type %></td>
        <td><%= link_to 'Edit', edit_exp_type_path(exp_type) %></td>
        <td><%= link_to 'Destroy', exp_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exp type', new_exp_type_path %>
